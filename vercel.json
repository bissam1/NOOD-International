{
     "version": 2,
     "builds": [
       {
         "src": "package.json",
         "use": "@vercel/next",
         "config": {
           "buildCommand": "next build",
           "installCommand": "npm install",
           "outputDirectory": ".next"
         }
       }
     ],
     "routes": [
       {
         "src": "/(.*)",
         "dest": "/"
       }
     ],
     "env": {
       "NODE_ENV": "production"
     }
   }
